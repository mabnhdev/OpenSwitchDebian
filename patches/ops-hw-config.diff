+#
+#  Thermal Description File for Alphanetworks SNH60A0 Switch.
+#  Based on the SNH60A0 description file.
+
+manufacturer:    Alphanetworks
+product_name:    SNH60A0-320T
+version:         null
+
+# import or reference devices list here
+
+thermal_info:
+      num_sensors:      2
+      polling_period:   5000   # 5 seconds
+      auto_shutdown:    True   # Auto shutdown if emergency alarm
+
+
+#  NOTE!!  The alarm and fan thresholds for sensors 1 and 2 were
+#          per Alphanetworks email guidance (July 18, 2014).  Other values
+#          are guesses.
+
+sensors:
+     -  number:        1
+        location:     "CPU Board Ambient"
+        device:       ambient
+        sensor_type:  lm75
+        alarm_thresholds:
+              emergency_on:         75
+              emergency_off:        70
+              critical_on:          65
+              critical_off:         62
+              max_on:               57.7
+              max_off:              52.7
+              min:                   0
+              low_crit:            -10
+        fan_thresholds:
+              max_on:               57.7     #  Set fan speed to MAX
+              max_off:              52.7
+              fast_on:              53         # Set fan speed to FAST
+              fast_off:             47.7
+              medium_on:            49.5       # Set fan speed to BRISK
+              medium_off:           42.7
+
+
+     -  number:        2
+        location:     "Mainboard Hotspot"
+        device:       hotspot
+        sensor_type:  lm75
+        thresholds:
+        alarm_thresholds:
+              emergency_on:         75
+              emergency_off:        70
+              critical_on:          65
+              critical_off:         62
+              max_on:               57.7
+              max_off:              52.7
+              min:                   0
+              low_crit:            -10
+        fan_thresholds:
+              max_on:               57.7     #  Set fan speed to MAX
+              max_off:              52.7
